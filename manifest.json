{
  "manifest_version": 2,
  "name": "Github Project Schedule",
  "version": "0.0.2.0",
  "content_scripts":
  [
    {
      "matches": [
        "https://github.com/*/*/projects/*",
        "https://github.com/*/*/issues/*"
      ],
      "css": [
        "flatpickr-material-red.css"
      ],
      "js": [
        "app_extention.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}